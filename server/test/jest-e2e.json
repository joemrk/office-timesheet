{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/u-prox-api/(.*)": "<rootDir>/../libs/u-prox-api/src/$1",
    "@app/u-prox-api": "<rootDir>/../libs/u-prox-api/src"
  }
}